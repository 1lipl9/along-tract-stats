function trk_exDTI_to_TrkVis_cfg_run_func(job)

niiFile = job.ex2trk_nii;
exTrkFile = job.extrk;
outputName = job.ex2trk_outputName;

nii = load_untouch_header_only(niiFile{1});

for aa = 1:numel(exTrkFile)
    [path, tit, ext] = fileparts(exTrkFile{aa});
    subOutputName = fullfile(path, [outputName, tit,
end
